#division of sub by k

class solution:
    def subaraydivision(self,A:List[int],k:int)-> int:
        rs=ans=0
        dp={0:1}
        for num in A:
            rs+=num
            key=rs%k

        if key in dp:
            ans+=dp[key]
            dp[key]+=1
        else:
            dp[key]=1
    return ans
